{
    "name": "hanaboso/app-store",
    "description": "App store for PipesFramework",
    "license": "Apache-2.0",
    "version": "1.6.2",
    "require": {
        "php": "^8.0",
        "hanaboso/pipes-php-sdk": "^1.6"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.4",
        "hanaboso/rest-bundle": "^1.1",
        "symfony/debug-bundle": "^v6.0",
        "symfony/security-bundle": "^v6.0",
        "symfony/serializer": "^6.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFAppStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFAppStoreTests\\": "tests/"
        }
    }
}
